{
  "name": "Airframe",
  "version": "2.2.0",
  "description": "A project starter kit",
  "repository": {
    "type": "git",
    "url": "https://github.com/planetary/airframe.git"
  },
  "author": "Planetary",
  "bugs": {
    "url": "https://github.com/planetary/airframe/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/planetary/airframe",
  "engines": {
    "node": ">=6.9.0",
    "npm": ">=3.10.8"
  },
  "scripts": {
    "prebuild": "rimraf build",
    "build": "cross-env webpack --env.prod -p",
    "postbuild": "imagemin assets/images/* --out-dir=build/images",
    "prebuild:docs": "rimraf build",
    "build:docs": "cross-env webpack --env.prod --env.docs -p",
    "postbuild:docs": "imagemin assets/images/* --out-dir=build/images",
    "prebuild:dev": "rimraf build",
    "build:dev": "cross-env webpack --env.dev --env.build",
    "postbuild:dev": "imagemin assets/images/* --out-dir=build/images",
    "dev": "cross-env webpack-dev-server --inline --hot --env.dev",
    "validate": "npm-run-all --parallel lint build",
    "lint": "eslint assets"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-register": "^6.18.0",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "css-modularscale": "^1.1.1",
    "eslint": "^3.2.2",
    "eslint-config-planetary": "^0.1.1",
    "extract-text-webpack-plugin": "2.0.0-beta.5",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "2.26.0",
    "image-webpack-loader": "^2.0.0",
    "imagemin-cli": "^3.0.0",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "npm-run-all": "^2.3.0",
    "postcss-cssnext": "^2.7.0",
    "postcss-import": "9.1.0",
    "postcss-loader": "^0.9.1",
    "postcss-mixins": "^5.0.0",
    "progress-bar-webpack-plugin": "^1.9.0",
    "pug": "2.0.0-beta8",
    "pug-loader": "^2.2.1",
    "pug-walk": "0.0.3",
    "purifycss-webpack-plugin": "^2.0.3",
    "reload-html-webpack-plugin": "^0.1.2",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "stylelint": "^7.7.1",
    "stylelint-webpack-plugin": "^0.4.2",
    "url-loader": "^0.5.7",
    "webpack": "2.2.0",
    "webpack-config-utils": "^2.0.0",
    "webpack-dev-server": "2.2.0",
    "webpack-md5-hash": "0.0.5"
  }
}
