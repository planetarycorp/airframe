{
  "name": "Airframe",
  "version": "1.1.1",
  "description": "A project starter kit",
  "repository": {
    "type": "git",
    "url": "https://github.com/planetary/airframe.git"
  },
  "author": "Planetary",
  "bugs": {
    "url": "https://github.com/planetary/airframe/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/planetary/airframe/blob/master/LICENSE"
    }
  ],
  "homepage": "https://github.com/planetary/airframe",
  "engines": {
    "node": ">=4.2.0",
    "npm": ">=2.1.18"
  },
  "scripts": {
    "prebuild": "rimraf build",
    "build": "webpack --env.prod -p",
    "postbuild": "imagemin assets/images/* --out-dir=build",
    "prebuild:dev": "rimraf build",
    "build:dev": "webpack --env.dev",
    "postbuild:dev": "imagemin assets/images/* --out-dir=build",
    "dev": "webpack-dev-server --env.dev --inline --hot",
    "validate": "npm-run-all --parallel lint build",
    "lint": "eslint assets"
  },
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^5.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-register": "^6.11.6",
    "copy-webpack-plugin": "^3.0.1",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.2.2",
    "eslint-config-planetary": "^0.1.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "image-webpack-loader": "^2.0.0",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "node-sass": "^3.8.0",
    "npm-run-all": "^2.3.0",
    "postcss-cssnext": "^2.7.0",
    "postcss-loader": "^0.9.1",
    "progress-bar-webpack-plugin": "^1.9.0",
    "pug": "^2.0.0-beta4",
    "pug-loader": "^2.2.1",
    "reload-html-webpack-plugin": "^0.1.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.20",
    "webpack-config-utils": "^2.0.0",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-validator": "^2.2.7"
  },
  "devDependencies": {}
}
